{
  "name": "prg-highlighter",
  "displayName": "PRG",
  "description": "HUASHU robot 编程语言vscode插件，支持高亮及补全",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Programming Languages",
    "Themes",
    "Snippets"
  ],
  "contributes": {
    "languages": [{
      "id": "prg",
      "aliases": ["PRG", "prg"],
      "extensions": [".PRG"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "prg",
      "scopeName": "source.PRG",
      "path": "./syntaxes/prg.tmLanguage.json"
    }],
    "themes": [
      {
        "label": "hsr-theme",
        "uiTheme": "vs-dark",
        "path": "./themes/hsr-theme-color-theme.json"
      }
    ],
    "snippets": [
      {
        "language": "prg",
        "path": "./snippets/hsr-snippets.json"
      }
    ]
  }
}
