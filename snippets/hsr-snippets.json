		],
		"description": "Log output to console"
	},
	"WHILE": {
		"scope": "prg",
		"prefix": "WHILE",
		"body": [
			"WHILE"
		],
		"description": "Log output to console"
	},
	"END WHILE": {
		"scope": "prg",
		"prefix": "END WHILE",
		"body": [
			"END WHILE"
		],
		"description": "Log output to console"
	},
	"BREAK": {
		"scope": "prg",
		"prefix": "BREAK",
		"body": [
			"BREAK"
		],
		"description": "Log output to console"
	},
	"RUN": {
		"prefix": "RUN",
		"body": [
			"RUN \"$1\""
		],
		"description": "description..."
	},
	"PAUSE": {
		"prefix": "PAUSE",
		"body": [
			"PAUSE"
		],
		"description": "description..."
	},
	"WORLDFRAME": {
		"prefix": "WORLDFRAME",
		"body": [
			"WORLDFRAME = $1"
		],
		"description": "description..."
	},
	"UTOOL_NUM": {
		"prefix": "UTOOL_NUM",
		"body": [
			"UTOOL_NUM = $1"
		],
		"description": "description..."
	},
	"UTOOL_FRAME": {
		"prefix": "UTOOL_FRAME",
		"body": [
			"UTOOL_FRAME = $1"
		],
		"description": "description..."
	},
	"SET_TOOL": {
		"prefix": "SET_TOOL",
		"body": [
			"SET_TOOL $1 LR[$2]"
		],
		"description": "description..."
	},
	"EX_UTOOL": {
		"prefix": "EX_UTOOL",
		"body": [
			"EX_UTOOL = $1"
		],
		"description": "description..."
	},
	"OPENSOCKET": {
		"scope": "prg",
		"prefix": "OPENSOCKET",
		"body": [
			"OPENSOCKET OPTIONS = $1 HANDLE = $2"
		],
		"description": "Log output to console"
	},
	"CLOSESOCKET": {
		"scope": "prg",
		"prefix": "CLOSESOCKET",
		"body": [
			"CLOSESOCKET HANDLE = $1"
		],
		"description": "Log output to console"
	},
	"ISHANDLEOPEN": {
		"scope": "prg",
		"prefix": "ISHANDLEOPEN",
		"body": [
			"ISHANDLEOPEN(${1:1})"
		],
		"description": "Log output to console"
	},
	"CONNECT": {
		"scope": "prg",
		"prefix": "CONNECT",
		"body": [
			"CONNECT(${1:1},${2:\"0.0.0.0\"},${3:30000})"
		],
		"description": "Log output to console"
	},
	"ACCEPT": {
		"scope": "prg",
		"prefix": "ACCEPT",
		"body": [
			"ACCEPT(${1:1},${2:\"0.0.0.0\"},${3:30000},R[${4:0}])"
		],
		"description": "Log output to console"
	},
	"SEND": {
		"scope": "prg",
		"prefix": "SEND",
		"body": [
			"SEND(${1:1},SR[$2])"
		],
		"description": "Log output to console"
	},
	"RECEIVE": {
	"prefix": "RECEIVE",
	"body": [
		"${1:R[] = }RECEIVE($2,SR[$3])"
	],
	"description": "description..."
	},
	"SOCKETMONITOR": {
	"prefix": "SOCKETMONITOR",
	"body": [
		"SOCKETMONITOR($1, $2)"
	],
	"description": "description..."
	},
}