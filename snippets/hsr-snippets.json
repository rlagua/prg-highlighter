{
	"prg init template": {
		"scope": "prg",
		"prefix": "#init",
		"body": [
			"<attr>",
			"VERSION:0",
			"GROUP:[${1|0,*|}]",
			"<end>",
			"<pos>",
			"<end>",
			"<program>",
			"",
			"$2",
			"<end>"
		],
		"description": "Log output to console"
	},
	"J": {
		"prefix": "J",
		"body": [
			"J ${1|P,JR,LR|}[$2]"
		],
		"description": "description..."
	},
	"L": {
		"prefix": "L",
		"body": [
			"L ${1|P,JR,LR|}[$2]"
		],
		"description": "description..."
	},
	"C": {
		"prefix": "C",
		"body": [
			"C ${1|P,JR,LR|}[$2] ${3|P,JR,LR|}[$4]"
		],
		"description": "description..."
	},
	"A": {
		"prefix": "A",
		"body": [
			"A ${1|P,JR,LR|}[$2]"
		],
		"description": "description..."
	},
	"DO": {
		"prefix": "DO",
		"body": [
			"DO[${1:R[]}] = ${2|ON,OFF|}"
		],
		"description": "description..."
	},
	"VDI": {
		"prefix": "VDI",
		"body": [
			"VDI[${1| ,R[]|}] = ${2|ON,OFF|}"
		],
		"description": "description..."
	},
	"AO": {
		"prefix": "AO",
		"body": [
			"AO[${1| ,R[]|}] = ${2: }"
		],
		"description": "description..."
	},
	"PLUSE": {
		"prefix": "PLUSE",
		"body": [
			"PLUSE DO[$1] TIME=$2"
		],
		"description": "description..."
	},
	"SET_TR": {
		"prefix": "SET_TR",
		"body": [
			"SET_TR not completed"
		],
		"description": "description..."
	},
	"WAIT TIME": {
		"prefix": "WAIT TIME",
		"body": [
			"WAIT TIME = $1"
		],
		"description": "description..."
	},
	"IFTHEN": {
		"scope": "prg",
		"prefix": "IFTHEN",
		"body": [
			"IF $1 THEN",
			"$2",
			"END IF"
		],
		"description": "Log output to console"
	},
	"IF GOTO": {
		"scope": "prg",
		"prefix": "IFGOTO",
		"body": [
			"IF $1,GOTO LBL[$2]"
		],
		"description": "Log output to console"
	},
	"IF CALL": {
		"scope": "prg",
		"prefix": "IFCALL",
		"body": [
			"IF $1,CALL \"$2\""
		],
		"description": "Log output to console"
	},
	"SELECT": {
		"scope": "prg",
		"prefix": "SELECT",
		"body": [
			"SELECT R[$1]"
		],
		"description": "Log output to console"
	},
	"CASE": {
		"scope": "prg",
		"prefix": "CASE",
		"body": [
			"CASE $1,GOTO LBL [$2]"
		],
		"description": "Log output to console"
	},
	"ELSECOTO": {
		"scope": "prg",
		"prefix": "ELSEGOTO",
		"body": [
			"ELSE,GOTO LBL[$1]"
		],
		"description": "Log output to console"
	},
	"CALL": {
		"scope": "prg",
		"prefix": "CALL",
		"body": [
			"CALL \"$1\""
		],
		"description": "Log output to console"
	},
	"GOTO": {
		"scope": "prg",
		"prefix": "GOTO",
		"body": [
			"GOTO LBL[$1]"
		],
		"description": "Log output to console"
	},
	"LBL": {
		"scope": "prg",
		"prefix": "LBL",
		"body": [
			"LBL[$1]"
		],
		"description": "Log output to console"
	},
	"FOR": {
		"scope": "prg",
		"prefix": "FOR",
		"body": [
			"FOR R[$1] = $2 TO $3"
		],
		"description": "Log output to console"
	},
	"FOR-BY": {
		"scope": "prg",
		"prefix": "FOR",
		"body": [
			"FOR R[$1] = $2 TO $3 BY $4"
		],
		"description": "Log output to console"
	},
	"END FOR": {
		"scope": "prg",
		"prefix": "END FOR",
		"body": [
