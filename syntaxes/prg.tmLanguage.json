{
	"name": "PRG",
	"scopeName": "source.PRG",
	"foldingStartMarker": "<attr>",
	"foldingStopMarker": "<program>",
	"comments": {
		"name": "comment.line.character.prg",
		"begin": "'",
		"end": "$",
		"patterns": [
			{
				"name": "constant.character.escape.prg",
				"match": ""
			}
		]
	},
	"patterns": [
		{
			"name": "cmd.equal.prg",
			"match": "(<>|=|>|<|>=|<=)"
		},
		{
			"name": "cmd.sport.prg",
			"match": "\\b(J|L|C|A|JUMP)\\b"
		},
		{
			"name": "cmd.io.prg",
			"match": "\\b(AO|PULSE|SET_TR|WAIT|TIME|VORD|SEC)\\b"
		},
		{
			"name": "cmd.condition.prg",
			"match": "\\b(IF|THEN|ELSE|END IF|SELECT|CASE)\\b"
		},
		{
			"name": "cmd.process.prg",
			"match": "\\b(CALL|GOTO|LBL)\\b"
		},
		{
			"name": "cmd.circle.prg",
			"match": "\\b(FOR|TO|BY|WHILE|END FOR|END WHILE|BREAK)\\b"
		},
		{
			"name": "cmd.programcontrol.prg",
			"match": "\\b(RUN|PAUSE|ABORT|END|LOAD|START)\\b"
		},
		{
			"name": "cmd.offset.prg",
			"match": "\\b(OFFSET_CONDITION|TOOL_OFFSET_CONDITION)\\b"
		},
		{
			"name": "cmd.dynamic.prg",
			"match": "\\b(PAYLOAD|COL_DETECT|COL_GUARD_ADJUST|SYNCEXTAX|COORD_NUM)\\b"
		},
		{
			"name": "cmd.coord.prg",
			"match": "\\b(WORLDFRAME|UTOOL_NUM|UFRAME_NUM|SET_TOOL|EX_UTOOL|SET_FRAME)\\b"
		},
		{
			"name": "cmd.sport_para.prg",
			"match": "\\b(LIMZ|ARCH_A|ARCH_B|VEL|ACC|DEC|VROT|J_VEL|J_ACC|J_DEC|L_VEL|L_ACC|L_DEC|L_VROT|C_VEL|C_ACC|C_DEC|C_VROT)\\b"
		},
		{
			"name": "cmd.traj_para.prg",
			"match": "\\b(CNT|CNT_TYPE|SMOOTH|WRISTJNT|TURN|TALARM|QZ_ADAPTANGLE)\\b"
		},
		{
			"name": "cmd.socket.prg",
			"match": "\\b(OPENSOCKET|OPTIONS|HANDLE|CLOSESOCKET|ISHANDLEOPEN|CONNECT|ACCEPT|SEND|RECEIVE|SOCKETMONITOR|BRECBUFEMPTY)\\b"
		},
		{
			"name": "cmd.modbus.prg",
			"match": "\\b(CLOSE_MODBUS_RTU|OPEN_MODBUS_RTU|OPEN_MODBUS_TCP|CLOSE_MODBUS_TCP|READ_MDB_COILSTATUS|READ_MDB_INPUTSTATUS|READ_MDB_INPUTREG|READ_MDB_HOLDREG|WRITE_MDB_COILSTATUS|WRITE_MDB_HOLDREG)\\b"
		},
		{
			"name": "cmd.other.prg",
			"match": "\\b(PRINTF|THROW|LEVEL|SAVE FILE|DELAY|WEAVE|NUM|TYPE|PLANE|AMP|FRE|SLAVE|MODE|CYCLE|MASK|MAC|HANDPICK|DO_INDEX|DI_INDEX|DO_VALUE|DI_VALUE|TIMES|OUT)\\b"
		},
		{
			"name": "cmd.level.prg",
			"match": "\\b(INFO|NOTE|WARNING|ERROR|FATAL)\\b"
		},
		{
			"name": "cmd.trace.prg",
			"match": "\\b(TRACE|SKIP|GET TP|TPABS)\\b"
		},
		{
			"name": "cmd.string.prg",
			"match": "\\b(TOSTR|TOVAL|STRLEN|FINDSTR|SUBSTR)\\b"
		},
		{
			"name": "math.prg",
			"match": "\\b(DIV|MOD|SQRT|SIN|COS|TAN|ASIN|ACOS|ATAN|ABS|TRUNC|ROUND)\\b"
		},
		{
			"name": "cmd.state.prg",
			"match": "(\\bON\\b|\\bOFF\\b|\\w+.PRG|\".*\")"
		},
		{
			"name": "operator.prg",
			"match": "\\b(AND|OR|DIV|MOD)\\b"
		},
		{
			"name": "constant.character.escape.prg",
			"match": ""
		},
		{
			"name": "cmd.register.prg",
			"match": "\\b(DO|DI|VDI|R|JR|LR|P|SR)\\b"
		},
		{
			"name": "character.prg",
			"match": "'.*"
		},
		{
			"name": "constant.prg",
			"match": "'\\d+"
		}
	]
}